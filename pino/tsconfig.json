{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./src-dist",
    "module": "nodenext",
    "target": "esnext",
    "moduleResolution": "nodenext",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "lib": ["esnext","DOM"],
    "types": ["node","electron"],
    "sourceMap": false,
    "declaration": false,
    "declarationMap": false,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "moduleDetection": "force"
  },
  "include": ["./src"],
  "exclude": ["node_modules", "build", "launcher-dist","launcher-renderer"]
}
